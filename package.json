  "license": "MIT",
  "description": "",
  "main": "./src/server.ts",
  "scripts": {
    "dev": "nodemon",
    "start": "ts-node src/server.ts",
    "lint": "eslint src/ --ext .js,.jsx,.ts,.tsx",
    "clean": "rm -rf dist build package",
    "ts-node": "ts-node",
    "docs": "typedoc --entryPoints src/server.ts",
    "build": "tsc -p tsconfig.json",
    "build:watch": "tsc -w -p tsconfig.json",
