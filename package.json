    "eslint": "^8.16.0",
    "jest": "^29.5.0",
    "nodemon": "^2.0.22",
    "ts-jest": "^28.0.3",
    "ts-node": "^10.9.1",
    "typedoc": "^0.22.15"
  },
  "proxy": "http://localhost:3001"
