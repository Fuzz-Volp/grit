    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^7.0.3",
    "morgan": "^1.10.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.10.0",
    "serve-favicon": "^2.5.0",
    "typescript": "^5.0.3",
    "web-vitals": "^3.3.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/jest": "^27.5.1",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^18.15.11",
